sha256sums=('SKIP')

package() {
  cd JSONRPC.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh JSONRPC "${pkgdir}" "${pkgname}" julia-git
}
